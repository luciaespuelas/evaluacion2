#include<iostream>
#include<stdio.h>
#include<stdlib.h>
#include<time.h>
//Este programa genera una matriz 3x3 rellena de
//nueros enteros entre el0 y el 9 con repeticion 
using namespace std;

int main(){
	long semillita; //long es como int pero para numeros largos 
	semillita=time(0);
	srand(semillita); //s.eed rand.om 
    cout<<"un numero al azar: "<<rand();
	return 0;
}
